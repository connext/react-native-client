{
  "name": "react-native-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "android": "node_modules/.bin/react-native run-android",
    "ios": "node_modules/.bin/react-native run-ios",
    "start": "node_modules/.bin/react-native start",
    "start:clean": "yarn run clean:metro && node_modules/.bin/react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
